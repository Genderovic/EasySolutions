
	<!--start-header--->
	<div class="header">
  	<!--start-wrap--->
  	<div class="wrap">
  			<!--start-logo--->
  			<div class="logo">
  				<a><img src="/images/logo.png" /></a>
  			</div>
  			<div class="top-nav">
          
  			</div>
  			<div class="clear"> </div>
  			<!--End-logo--->
  		  </div>
  		<!--End-header--->
  	</div>
  	<div class="clear"> </div>
  	<div class="banner-slider">
  	<div class="wrap box" style="width: 37%">
      <div class="contact-form" >
        
        
        
        <h2 class="rotulo" >INICIAR SESIÓN</h2>
           <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
            <div class="field">
              <span><label><%= f.label :email, 'Correo' %><br /></label></span>
              <%= f.text_field :email, autocomplete: "off", :class => "textfield" %>
            </div>
          
            <div class="field">
              <span><label><%= f.label :password, 'Contraseña' %><br /></label></span>
              <%= f.password_field :password, autocomplete: "off", :class => "textfield" %>
            </div>
          
            <div align="center">
              <%= f.submit "Entrar" %>
            </div>
          <% end %>
        
        <span align="center"><%= render "devise/shared/links" %></span>
        



   
        
      </div>
      <div class="clear"> </div>
  </div>
</div>

	<!--End-wrap--->
